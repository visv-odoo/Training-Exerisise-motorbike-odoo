<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="registry_submenu_action" model="ir.actions.act_window">
        <field name="name">motorcycle registry</field>
        <field name="res_model">motorcycle.registry</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                create registration
            </p>
        </field>
    </record>

    <menuitem
        id="registry_menu"
        name="Motorcycle Registry"
        web_icon="Motorcycle_Registry,static/description/icon.png"
        sequence="1">
        <menuitem
            id="menu_registry_sub"
            name=""
            sequence="1">
            <menuitem
                id="motorcycle_registry_menu"
                name="name"
                action="registry_submenu_action"
                sequence="2">
            </menuitem>
        </menuitem>
    </menuitem>
    

</odoo>
